{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "crons": [
    {
      "path": "/api/indexnow",
      "schedule": "0 9 * * *"
    }
  ],
  "redirects": [
    {
      "source": "/accessbility-policy",
      "destination": "/accessibility-policy",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        { "type": "host", "value": "www.tntdump.com" }
      ],
      "destination": "https://tntdump.com/$1",
      "permanent": true
    }
  ]
}
