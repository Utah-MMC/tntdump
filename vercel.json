{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        { "type": "host", "value": "www.tntdump.com" }
      ],
      "destination": "https://tntdump.com/$1",
      "permanent": true
    }
  ]
}
