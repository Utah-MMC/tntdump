{
  "name": "tnt-dumpsters",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "npm run sitemap",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "sitemap": "run-s sitemap:index sitemap:types sitemap:indexnow",
    "sitemap:types": "node scripts/automation/generate-type-sitemaps.mjs",
    "sitemap:index": "next-sitemap --config next-sitemap.config.js",
    "sitemap:indexnow": "node scripts/submit-indexnow.js",
    "sitemap:local": "run-s sitemap:index sitemap:types",
    "indexnow": "node scripts/submit-indexnow.js",
    "indexnow:all": "node scripts/submit-indexnow.js --all",
    "generate:cities": "ts-node scripts/generate-city-pages.ts data/cities/cities.csv",
    "validate:cities": "ts-node scripts/validate-city-pages.ts",
    "pre-audit": "node scripts/pre-ahrefs-audit.js",
    "audit:lighthouse": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json --chrome-flags=\"--incognito --headless\"",
    "lighthouse:view": "lighthouse http://localhost:3000 --chrome-flags=\"--incognito\" --view",
    "apply:lighthouse-fixes": "node scripts/apply-lighthouse-fixes.js",
    "audit:production": "lighthouse https://tntdump.com --output=json --output-path=./lighthouse-prod-report.json --chrome-flags=\"--incognito --headless\"",
    "optimize:performance": "node scripts/optimize-performance.js",
    "audit:ahrefs": "node scripts/pre-ahrefs-audit.js",
    "fix:audit": "node scripts/fix-audit-issues.js",
    "images:optimize-rename": "node scripts/optimize-rename-images.js",
    "audit:wcag": "node scripts/fix-wcag-issues.mjs"
  },
